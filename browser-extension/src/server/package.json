{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ../../dist/server/index.js",
    "clean": "rm -rf ../../dist/server/",
    "copy": "cpy --parents './' '!./node_modules/' '!survey' '../../dist/server/' && cpy --parents './**/*.svg' '!./node_modules/' '../../dist/server/'",
    "prebuild": "npm run clean && npm run copy",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "prewatch": "npm run clean && npm run copy",
    "watch": "NODE_ENV=development webpack --config webpack.config.js -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
