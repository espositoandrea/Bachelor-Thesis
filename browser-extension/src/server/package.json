    "awesome-typescript-loader": "^5.2.1",
    "bootstrap": "^4.4.1",
    "cpy-cli": "^3.1.0",
    "css-loader": "^3.4.2",
    "ejs-loader": "^0.3.5",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
